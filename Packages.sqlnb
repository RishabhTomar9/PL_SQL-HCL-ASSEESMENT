    value: "CREATE TABLE documents (\r

      doc_id NUMBER PRIMARY KEY,\r

      doc_name VARCHAR2(100),\r

      doc_content CLOB,\r

      doc_file BLOB,\r

      created_date DATE DEFAULT SYSDATE\r

      );"
    languageId: oracle-sql
  - kind: 2
    value: "DECLARE\r

      v_doc_content CLOB := 'This is a large text document...';\r

      BEGIN\r

      INSERT INTO documents (doc_id, doc_name, doc_content)\r

      VALUES (1, 'My Document', v_doc_content);\r

      COMMIT;\r

      END;\r

      /\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "CREATE OR REPLACE DIRECTORY my_dir AS 'E:\\';\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "GRANT READ ON DIRECTORY my_dir TO hemant;\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "DECLARE\r

      \    v_blob   BLOB;\r

      \    v_bfile  BFILE;\r

      BEGIN\r

      \    INSERT INTO documents (doc_id, doc_name, doc_file)\r

      \    VALUES (2, 'my self.jpg', EMPTY_BLOB())\r

      \    RETURNING doc_file INTO v_blob;\r

      \r

      \    v_bfile := BFILENAME('MY_DIR', 'my self.jpg');\r

      \r

      \    DBMS_LOB.FILEOPEN(v_bfile, DBMS_LOB.FILE_READONLY);\r

      \r

      \    DBMS_LOB.LOADFROMFILE(\r

      \        dest_lob => v_blob,\r

      \        src_lob  => v_bfile,\r

      \        amount   => DBMS_LOB.GETLENGTH(v_bfile)\r

      \    );\r

      \r

      \    DBMS_LOB.FILECLOSE(v_bfile);\r

      \r

      \    COMMIT;\r

      END;\r

      /\r\n"
    languageId: oracle-sql
