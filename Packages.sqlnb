    languageId: oracle-sql
  - kind: 2
    value: "create table stu (\r

      \   stu_id     number primary key,\r

      \   stu_name   varchar2(50),\r

      \   stu_dept   varchar2(30),\r

      \   marks      number,\r

      \   created_on date\r

      );"
    languageId: oracle-sql
  - kind: 2
    value: "create or replace trigger trg_stu_bi before\r

      \   insert on stu\r

      \   for each row\r

      begin\r

      \   :new.created_on := sysdate;\r

      end;\r

      /"
    languageId: oracle-sql
  - kind: 2
    value: "create or replace package stu_pkg as\r

      \   procedure add_student (\r

      \      p_id    number,\r

      \      p_name  varchar2,\r

      \      p_dept  varchar2,\r

      \      p_marks number\r

      \   );\r

      \r

      \   procedure show_students;\r

      end stu_pkg;\r

      /"
    languageId: oracle-sql
  - kind: 2
    value: "create or replace package body stu_pkg as\r

      \r

      \   procedure add_student (\r

      \      p_id    number,\r

      \      p_name  varchar2,\r

      \      p_dept  varchar2,\r

      \      p_marks number\r

      \   ) is\r

      \   begin\r

      \      insert into stu (\r

      \         stu_id,\r

      \         stu_name,\r

      \         stu_dept,\r

      \         marks\r

      \      ) values ( p_id,\r

      \                 p_name,\r

      \                 p_dept,\r

      \                 p_marks );\r

      \   end add_student;\r

      \r

      \r

      \   procedure show_students is\r

      \   begin\r

      \      for r in (\r

      \         select *\r

      \           from stu\r

      \          order by stu_id\r

      \      ) loop\r

      \         dbms_output.put_line(r.stu_id\r

      \                              || ' | '\r

      \                              || r.stu_name\r

      \                              || ' | '\r

      \                              || r.stu_dept\r

      \                              || ' | '\r

      \                              || r.marks\r

      \                              || ' | ' || to_char(\r

      \            r.created_on,\r

      \            'DD-MON-YYYY'\r

      \         ));\r

      \      end loop;\r

      \   end show_students;\r

      \r

      end stu_pkg;\r

      /"
